{
  "name": "swc-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.44",
    "@types/node": "^20.2.5",
    "@types/zeromq": "^5.2.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.3"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:watch\" \"nodemon dist/index.js\"",
    "watch": "swc src --out-dir dist --watch --source-maps inline",
    "build": "swc src --out-dir dist --source-maps inline",
    "build:tsp": "tspc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fastify": "^4.21.0",
    "fastify-socket.io": "^4.0.0",
    "socket.io": "^4.7.2",
    "ts-patch": "^3.0.2",
    "ts-runtime-checks": "^0.3.3",
    "whatsapp-web.js": "^1.22.1",
    "zeromq": "5.3.1"
  }
}
